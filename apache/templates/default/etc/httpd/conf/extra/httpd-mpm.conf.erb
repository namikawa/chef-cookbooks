<IfModule mpm_event_module>
    StartServers            <%= node['apache']['mpm_event']['StartServers'] %>
    ServerLimit             <%= node['apache']['mpm_event']['ServerLimit'] %>
    MinSpareThreads         <%= node['apache']['mpm_event']['MinSpareThreads'] %>
    MaxSpareThreads         <%= node['apache']['mpm_event']['MaxSpareThreads'] %>
    ThreadsPerChild         <%= node['apache']['mpm_event']['ThreadsPerChild'] %>
    ThreadLimit             <%= node['apache']['mpm_event']['ThreadLimit'] %>
    MaxRequestWorkers       <%= node['apache']['mpm_event']['MaxRequestWorkers'] %>
    MaxConnectionsPerChild  <%= node['apache']['mpm_event']['MaxConnectionsPerChild'] %>
</IfModule>

